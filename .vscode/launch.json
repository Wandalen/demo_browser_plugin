{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PluginRS",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/plugin_rs"
            ],
            "outFiles": [
                "${workspaceFolder}/plugin_rs/out/**/*.js"
            ],
            "preLaunchTask": "npm: watch"
        },
        {
            "name": "PluginTS",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/plugin_ts"
            ],
            "outFiles": [
                "${workspaceFolder}/plugin_ts/out/**/*.js"
            ],
            "preLaunchTask": "npm: watch"
        }
    ]
}
